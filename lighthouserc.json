{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "settings": {
        "formFactor": "mobile",
        "throttling": {
          "rttMs": 40,
          "throughputKbps": 10240,
          "cpuSlowdownMultiplier": 1,
          "requestLatencyMs": 0,
          "downloadThroughputKbps": 0,
          "uploadThroughputKbps": 0
        },
        "screenEmulation": {
          "mobile": true,
          "width": 375,
          "height": 667,
          "deviceScaleFactor": 2,
          "disabled": false
        }
      },
      "url": [
        "http://localhost:5175/puppy-weight-watch/",
        "http://localhost:5175/puppy-weight-watch/calculator",
        "http://localhost:5175/puppy-weight-watch/weight-tracker",
        "http://localhost:5175/puppy-weight-watch/puppy-book-landing"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", {"minScore": 0.85}],
        "categories:accessibility": ["error", {"minScore": 0.9}],
        "categories:best-practices": ["warn", {"minScore": 0.9}],
        "categories:seo": ["warn", {"minScore": 0.85}],
        "viewport": "error",
        "uses-responsive-images": "warn",
        "tap-targets": "error",
        "content-width": "error"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
